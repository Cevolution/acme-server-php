{
  "name": "cevolution/acme-server-php",
  "version": "v1.3.7",
  "description": "ACME Server Library for PHP",
  "homepage": "https://cevolution.co.uk",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "authorization",
    "auth",
    "acme",
    "ciam"
  ],
  "authors": [
    {
      "name": "Cevolution",
      "email": "support@cevolution.co.uk",
      "homepage": "https://cevolution.co.uk/"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "ext-openssl": "*",
    "lcobucci/clock": "^3.3",
    "lcobucci/jwt": "^5.5",
    "league/oauth2-client": "^2.9",
    "nyholm/psr7-server": "^1.1",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1",
    "phpstan/phpstan": "^1",
    "phpunit/phpunit": "^9",
    "yoast/phpunit-polyfills": "^1"
  },
  "config": {
    "classmap-authoritative": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "pestphp/pest-plugin": true,
      "wikimedia/composer-merge-plugin": true,
      "php-http/discovery": false,
      "ergebnis/composer-normalize": true
    },
		"platform": {
			"php": "8.3"
		}
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
